      <Button fx:id="login" contentDisplay="CENTER" maxHeight="30.0" maxWidth="50.0" mnemonicParsing="false" onAction="#loginAction" text="Login" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="5" GridPane.vgrow="ALWAYS" />
      <Label fx:id="message" maxWidth="1.7976931348623157E308" prefHeight="5.0" prefWidth="205.0" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.hgrow="NEVER" GridPane.rowIndex="2" GridPane.valignment="CENTER" GridPane.vgrow="NEVER" />
      <Button fx:id="clear" mnemonicParsing="false" onAction="#clear" text="Clear" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="5" />
      <Label text="LOGIN" textFill="#dd13ab" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="1">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Button fx:id="signup" mnemonicParsing="false" onAction="#signup" style="-fx-background-color: FFFFFF;" text="Sign Up" textFill="#1700ff" underline="true" GridPane.halignment="RIGHT" GridPane.rowIndex="5">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="60.0" right="80.0" top="50.0" />
   </padding>
